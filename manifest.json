{
  "manifest_version": 2,

  "name": "Harvest-Shortcuts",
  "version": "1.0",
  "description": "This extension adds keyboard shortcuts to Harvest.",

  "browser_action": {
      "default_icon": "icon.png"
  },

  "content_scripts": [
    {
      "matches": ["https://vigetlabs.harvestapp.com/time*"],
      "js": ["jquery.js", "keyboard_shortcuts.js"]
    }
  ]
}
